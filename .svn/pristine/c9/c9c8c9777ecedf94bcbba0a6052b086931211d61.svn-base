<div ng-controller="userRegisterCtrl">
    <div class="row">
        <div class="col-md-2 col-md-offset-1"><a href="http://localhost/shipping"><img src="img/yiliansudi.png" class="yilian"/></a></div>
        <div class="col-md-2 form-title">欢迎注册</div>
    </div>
    <br>
    <br>
    <div class="col-md-5 col-md-offset-4 form-border">
        <div class="fl-bun">
            <a class="btn btn-hls btn-xs" href="http://localhost/shipping/user.html#/user/login">已有帐号，立即登陆</a>
        </div>
        <form action="">
            <div class="form-group">
                <label class="col-md-3 control-label register-label"> <span class="font-red">*</span> 用户名:</label>
                <div class="col-md-8"><input type="text" class="form-control"></div>
            </div>
            <br><br><br>
            <div class="form-group">
                <label class="col-md-3 control-label register-label"><span class="font-red">*</span> 登录密码:</label>
                <div class="col-md-8"><input type="password" class="form-control" id="pass"></div>
            </div>
            <br>
            <div class="col-md-offset-3 col-md-8 text-right">
                <small>安全程度:
                    <span class="strength1">弱</span>&nbsp;
                    <span class="strength2">中</span>&nbsp;
                    <span class="strength3">强</span>
                </small>
            </div>
            <br/><br/>
            <div class="form-group">
                <label class="col-md-3 control-label register-label"><span class="font-red">*</span> 确认密码:</label>
                <div class="col-md-8"><input type="password" class="form-control"></div>
            </div>
            <br><br>
            <div class="form-group">
                <label class="col-md-3 control-label register-label"><span class="font-red">*</span> 手机号码:</label>
                <div class="col-md-8"><input type="text" class="form-control"></div>
            </div>
            <br><br>
            <div class="form-group">
                <label class="col-md-3 control-label register-label"><span class="font-red">*</span> 手机验证:</label>
                <div class="col-md-3"><input type="text" class="form-control"></div>
                <div class="col-md-4 pt10"><a href="#">免费获取验证码</a></div>
            </div>
            <br><br>

            <div class="form-group">
                <label class="col-md-3 control-label register-label">邮箱:</label>
                <div class="col-md-8"><input type="text" class="form-control"></div>
            </div>
            <br><br>

            <div class=" text-left pl-15">
                <input type="checkbox">
                <small>我已阅读并同意《<a href="#">易联速递用户注册协议</a>》</small>
            </div>
            <br>
            <div class="col-md-offset-2 col-md-8">
                <button type="submit" class="btn btn-hls btn-block">立即注册</button>
            </div>
        </form>
    </div>
</div>